<% link = pagy_link_proc(pagy) -%>
<%#                            -%><ul aria-label="pager" class="pagy_nav pagination" role="navigation">
<% if pagy.prev                -%>  <li class="pagination-arrows material-icons"><%== link.call(pagy.prev, 'chevron_left', 'aria-label="previous"') %></li>
<% else                        -%>  <li class="pagination-arrows disabled material-icons"><%== 'chevron_left' %></li>
<% end                         -%>
<% pagy.series.each do |item| # series example: [1, :gap, 7, 8, "9", 10, 11, :gap, 36] -%>
<%   if    item.is_a?(Integer) -%>  <li class="pagination-number pagy"><%== link.call(item) %></li>
<%   elsif item.is_a?(String)  -%>  <li class="pagination-number current"><%= item %></li>
<%   elsif item == :gap        -%>  <li class="pagination-number gap"><%== pagy_t('pagy.nav.gap') %></li>
<%   end                       -%>
<% end                         -%>
<% if pagy.next                -%>  <li class="pagination-arrows material-icons"><%== link.call(pagy.next, 'chevron_right', 'aria-label="next"') %></li>
<% else                        -%>  <li class="pagination-arrows disabled material-icons"><%== 'chevron_right' %></li>
<% end                         -%>
<%#                            -%></ul>
