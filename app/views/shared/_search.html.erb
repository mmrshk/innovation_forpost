<%= search_form_for @q , url: articles_url, :html => { class: 'tag-search vertical short' } do |f| %>
  <% @tags.each do |tag| %>
    <%= label_tag '', class: 'tag-search-item' do %>
      <%= check_box_tag "q[tags_name_cont_any][]", tag.name, false %>
      <%= tag.name %>
    <% end %>
  <% end %>
  <%= f.submit "Search", class: 'button-search' %>
<% end %>
