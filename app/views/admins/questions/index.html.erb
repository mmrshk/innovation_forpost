<h1>Questions</h1>

<% @questions.each do |question| %>
  <h2><%= question.title %></h2>


  <p>
    <%= question.body %>
    <%= question.user_name %>
    <%= question.user_email %>
  </p>
  <%= link_to 'Show', admins_question_path(question) %>
  <%= link_to 'Edit', edit_admins_question_path(question) %>
  <%= link_to 'Destroy', admins_question_path(question), data: {method: :delete, confirm: 'Are you sure?'} %>
<% end %>
