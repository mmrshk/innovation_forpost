<!DOCTYPE html>
<html>
  <head>
    <title>InnovationForpost</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= yield :js %>
    <%= stylesheet_link_tag 'application', media: 'all', 'data-turbolinks-track': 'reload' %>
