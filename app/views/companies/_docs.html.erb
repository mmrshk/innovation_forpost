<div class="wrapper-docs">
  <div class="docs-headline company-headline"><%= t('docs.headline') %></div>
  <div class="docs-container">
    <% documents.each do |document|%>
      <%= link_to admins_attachment_path(document) do %>
        <div class="docs-block box">
          <div class="docs-image-preview">
            <span><i class="material-icons large">insert_drive_file</i></span>
          </div>
          <div class="docs-title-block"><%= document.name %></div>
        </div>
      <% end %>
    <% end %>
  </div>
  <div class="button-load-more btn-docs "><%= t('docs.button_more') %></div>
</div>
